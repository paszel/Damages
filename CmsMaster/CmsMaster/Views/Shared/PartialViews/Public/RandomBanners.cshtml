@model List<CmsMaster.Models.Cooperator>

@foreach (var item in Model)
{
    <span>
        <a href="@item.UrlAddress" title="@string.Format("{0}\n{1}",item.Title,item.Description)">
            <img src="@Url.Action("GetImage", "Home", new { path = item.FilePath }) " alt="baner" width="100" height="100"></a>
    </span>
}
